/ Loop for
for (int i = 0; i < 5; i++) {
    System.out.println("NÃºmero: " + i);
}

// Loop while
int contador = 0;
while (contador < 5) {
    System.out.println("Contador: " + contador);
    contador++;
}

// Loop do-while
int x = 0;
do {
    System.out.println("Valor de x: " + x);
    x++;
} while (x < 5);
